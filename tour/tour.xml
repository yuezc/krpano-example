		<layer style="mapspot" name="spot4" title="博物馆" x="112" y="88" zorder="1" />
		<layer style="mapspot" name="spot5" title="图书馆" x="222" y="68" zorder="1" />
		<layer style="mapspot" name="spot6" title="规划馆" x="512" y="78" zorder="1" />
		<layer style="mapspot" name="spot7" title="市民中心" x="712" y="88" zorder="1" />
		<layer style="mapspot" name="spot7" title="双创中心" x="212" y="388" zorder="1" />
	</layer>

