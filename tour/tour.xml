		   url="skin/camicon.png"
		   align="lefttop" edge="bottom" scale="0.9"
		   onover.addevent="tween(scale,0.55,0.2);"
		   onout.addevent="tween(scale,0.5,0.2);"
		   tooltip="111"
		   onclick="mapspot_loadscene();"
	/>

